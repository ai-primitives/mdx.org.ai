{
  "name": "mdx.org.ai",
  "version": "0.1.0",
  "description": "TypeScript types for MDX.org.ai file types and documentation",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "extract-docs": "NODE_OPTIONS='--loader ts-node/esm' ts-node --esm scripts/extract-docs.ts",
    "prebuild": "mkdir -p src/generated && pnpm extract-docs && NODE_OPTIONS='--loader ts-node/esm' ts-node --esm scripts/generate-types.ts",
    "build": "tsc",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "mdx",
    "typescript",
    "types",
    "documentation"
  ],
  "author": "MDX.org.ai",
  "license": "MIT",
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "^20.11.16",
    "glob": "^11.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
